{
  "name": "@accio-ui/react-tsx",
  "version": "0.2.1",
  "description": "Headless primitives for react",
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest -u",
    "build": "pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "dev": "pnpm build:swc -w",
    "types": "tsc --emitDeclarationOnly",
    "lint": "tsc --noEmit && rome check --apply-suggested . && rome format --write .",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeiaccio/react-tsx.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Alexei Accio <alexei.accio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexeiaccio/react-tsx/issues"
  },
  "homepage": "https://github.com/alexeiaccio/react-tsx#readme",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*/index.js",
      "./dist/*.js"
    ]
  },
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*/index",
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*/index.d.ts",
          "dist/*.d.ts"
        ]
      }
    }
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.38",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "happy-dom": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^11.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  }
}
