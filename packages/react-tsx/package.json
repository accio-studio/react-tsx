{
  "name": "@accio-ui/react-tsx",
  "version": "0.2.4",
  "description": "Headless primitives for react",
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "tsc && rome check --apply-suggested . && rome format --write .",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeiaccio/react-tsx.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Alexei Accio <alexei.accio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexeiaccio/react-tsx/issues"
  },
  "homepage": "https://github.com/alexeiaccio/react-tsx#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/index.d.ts"
        ]
      }
    }
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "happy-dom": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^11.0.0",
    "tsup": "^6.6.3",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  },
  "peerDependencies": {
    "react": "^18"
  }
}
