{
  "name": "@accio-ui/ui",
  "version": "1.0.0",
  "description": "Design system with react",
  "scripts": {
    "test:e2e": "playwright test",
    "fwoosh": "fwoosh dev",
    "fwoosh:build": "fwoosh build",
    "fwoosh:serve": "fwoosh serve",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "tsc && rome check --apply-suggested . && rome format --write .",
    "clean": "rm -rf dist && rm -rf out && rm -rf .turbo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeiaccio/react-tsx.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Alexei Accio <alexei.accio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexeiaccio/react-tsx/issues"
  },
  "homepage": "https://github.com/alexeiaccio/react-tsx#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/index.d.ts"
        ]
      }
    }
  },
  "dependencies": {
    "@accio-ui/colors": "workspace:*",
    "@accio-ui/react-tsx": "workspace:*",
