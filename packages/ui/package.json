    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "cva": "npm:class-variance-authority@^0.4.0",
    "fwoosh": "^0.0.126",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^11.0.0",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.2.7",
    "tsup": "^6.6.3",
    "typescript": "^5.0.2"
