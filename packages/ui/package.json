    "@react-aria/utils": "^3.15.0"
  },
  "devDependencies": {
    "@csstools/postcss-oklab-function": "^2.1.0",
    "@fwoosh/components": "0.0.126",
    "@fwoosh/decorator-centered": "^0.0.126",
    "@fwoosh/panel-actions": "0.0.126",
    "@fwoosh/panel-designs": "0.0.126",
    "@fwoosh/panel-props": "0.0.126",
    "@fwoosh/panel-source": "0.0.126",
    "@fwoosh/panel-story-description": "0.0.126",
    "@fwoosh/react": "0.0.126",
    "@fwoosh/styling": "0.0.126",
    "@fwoosh/test": "^0.0.126",
    "@fwoosh/tool-github": "0.0.126",
    "@fwoosh/tool-measure": "0.0.126",
    "@fwoosh/tool-viewport": "0.0.126",
    "@fwoosh/tool-zoom": "0.0.126",
